% layout 'default';

% content_for 'css' => begin
%= stylesheet '/css/show.css'
% end

% content_for 'js' => begin
<script src="/js/prettify/run_prettify.js?skin=desert"></script>
% end

<div class="row">
	<div class="col-xs-12">
		<h1><%= $title // '' %><a href="/p" class="btn btn-default pull-right">Create new paste</a></h1>
		<pre class="prettyprint linenums"><code><%= stash('content') %></code></pre>
	</div>
</div>
